# Database (automatically set by Replit)
DATABASE_URL=postgresql://user:password@localhost:5432/database

# OpenAI API Key
# Get yours from: https://platform.openai.com/account/api-keys
# For Replit: Uses AI_INTEGRATIONS_OPENAI_API_KEY automatically
# For Vercel: Set OPENAI_API_KEY as an environment variable
OPENAI_API_KEY=sk_your_openai_api_key_here

# Optional: Custom OpenAI Base URL (for Replit AI Integrations only)
# AI_INTEGRATIONS_OPENAI_BASE_URL=https://api.openai.com/v1
# AI_INTEGRATIONS_OPENAI_API_KEY=your_replit_ai_key
